<h1>Add article</h1>

<form action="/add-article" method="POST">
    <div class="input-field">
        <input id="title" type="text" class="validate">
        <label for="first_name">Title</label>
    </div>
    <div class="input-field">
        <input id="time" type="text" class="validate">
        <label for="first_name">Time</label>
    </div>
    <div class="input-field">
        <input id="url" type="text" class="validate">
        <label for="first_name">URL image</label>
    </div>
    <div class="input-field">
        <input id="text" type="text" class="validate">
        <label for="first_name">Text</label>    
    </div>

</form>